#include<iostream>
using namespace std;
int a[3][3];
int row_sum=0;
int column_sum=0;
int main()
	{
	
	for(int i=0;i<3;i++){
for(int j=0;j<3;j++){
cin>>a[i][j];
}
for(int l=0;l<3;i++)
{
	row_sum+=a[3/2][l];
	cout<<row_sum;
}
for(int m=0;m<3;m++)
{
	column_sum+=a[m][3/2];
}
cout<<column_sum;
}
return 0;
}
